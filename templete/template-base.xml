<?xml version="1.0" encoding="UTF-8"?>
<list size="10">

	<!-- 布尔型数据两种展示方式 -->
	<boolean key="isChecked" type="default" />
	<boolean key="isSelected" type="number" />

	<string key="title" max-length="128" />
	<string key="article" />

	<!-- 数值型数据不允许小于零 -->
	<integer key="age" min="0" max="200" />
	<!-- 指定范围内的数值 -->
	<integer key="score" min="0" max="100" />
	<!-- 指定范围内的数值 -->
	<integer key="percent" min="0" max="100" precision="2" />
	<!-- max min 的优先级 更高 如存在 values 不要设置 min 和 max -->
	<integer key="inNum" values="10,21,32,43,54" />

	<double key="PI" min="0" />

</list>